<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:conv="clr-namespace:ObjectDesign.Wpf.Converters"
                    xmlns:od="clr-namespace:Ao.ObjectDesign;assembly=Ao.ObjectDesign">
    <conv:TypeEnumConverter x:Key="TypeEnumConverter"/>
    <DataTemplate x:Key="ObjectDesign.Enum"
                  DataType="{x:Type od:PropertyProxy}">
        <ComboBox ItemsSource="{Binding Type,Converter={StaticResource TypeEnumConverter}}"
                  SelectedItem="{Binding Instance,Mode=TwoWay}"/>
    </DataTemplate>
</ResourceDictionary>