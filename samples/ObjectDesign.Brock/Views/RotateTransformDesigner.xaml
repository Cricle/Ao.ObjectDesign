<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:w="https://github.com/Cricle/Ao.ObjectDesign.Wpf"
                    xmlns:l="https://github.com/Cricle/Ao.Lang">
    <DataTemplate x:Key="ObjectDesign.RotateTransformDesigner"
                  DataType="{x:Type w:WpfTemplateForViewBuildContext}">
        <StackPanel Orientation="Vertical">
            <TextBlock Text="旋转"/>
            <StackPanel DataContext="{Binding PropertyVisitor}"
                        HorizontalAlignment="Left"
                        Orientation="Vertical">
                <Border DataContext="{Binding Value}">
                    <TextBox Text="{Binding Angle,Mode=TwoWay}"/>
                </Border>
            </StackPanel>
        </StackPanel>
    </DataTemplate>

</ResourceDictionary>